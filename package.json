{
  "name": "react-apollo-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.0.0-beta.23",
    "graphql": "14.5.8",
    "lodash": "4.17.15",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-scripts": "3.4.0",
    "theme-ui": "0.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
