    "@testing-library/react": "9.4.0",
    "@testing-library/user-event": "7.1.2",
    "apollo-link-error": "1.1.12",
    "apollo-link-schema": "1.2.4",
    "apollo-mocked-provider": "3.0.2",
    "faker": "4.1.0",
    "graphql-tools": "4.0.6",
    "isomorphic-unfetch": "3.0.0",
    "jest": "24.9.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.js"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "transform": {
